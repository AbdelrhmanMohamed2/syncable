{
    "name": "AbdelrhmanMohamed2/syncable",
    "description": "A Laravel package for syncing data between multiple Laravel projects",
    "keywords": [
        "laravel",
        "syncable",
        "api",
        "sync",
        "data-synchronization"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdelrhman Mohamed",
            "email": "abdo.mohamed22298@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0||^11.0|^12.0",
        "illuminate/database": "^10.0||^11.0|^12.0",
        "illuminate/http": "^10.0||^11.0|^12.0",
        "illuminate/config": "^10.0||^11.0|^12.0",
        "illuminate/events": "^10.0||^11.0|^12.0",
        "illuminate/queue": "^10.0||^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Syncable\\": "src/Syncable/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Syncable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Syncable\\Providers\\SyncableServiceProvider"
            ],
            "aliases": {
                "Syncable": "Syncable\\Facades\\Syncable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}